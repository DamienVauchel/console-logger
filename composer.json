{
    "name": "scoobydam/console-logger",
    "description": "Simple and light PHP console logger",
    "keywords": ["console", "logger", "print", "echo", "easy", "simple", "light"],
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien",
            "email": "damien.vauchel@pm.me"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "psr/log": "^1.1"
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "ScoobyConsoleLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoobyConsoleLogger\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpro/grumphp": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12.83",
        "phpunit/phpunit": "^9.5"
    }
}
